{
  "name": "mdapi2",
  "version": "0.1.0",
  "description": "MDAPI but better. And newer.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm i && git submodule init && git submodule update",
    "update-sub": "git submodule update --remote src/gentools",
    "update": "git pull origin main --recurse-submodules && pm2 restart mdapi2",
    "gen:schemas": "ts-node src/schema.generate.ts",
    "gen:services": "ts-node src/service.generate.ts",
    "gen:all": "npm run gen:schemas && npm run gen:services",
    "gen:routes": "ts-node src/routes.generate.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "start": "ts-node src/index.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.2.186",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "@types/passport": "^1.0.7",
    "@types/ws": "^8.5.3",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "code-block-writer": "^11.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "dotenv-defaults": "^5.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.0.2",
    "mongoose": "^6.2.10",
    "passport": "^0.5.2",
    "ts-morph": "^14.0.0",
    "tsconfig-paths": "^3.14.1",
    "ws": "^8.5.0"
  }
}
