{
  "name": "mdapi2",
  "version": "0.1.0",
  "description": "MDAPI but better. And newer.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "git pull origin main --recurse-submodules && pm2 restart mdapi2",
    "gen:schemas": "ts-node src/schema.generate.ts",
    "gen:services": "ts-node src/service.generate.ts",
    "gen:all": "npm run gen:schemas && npm run gen:services",
    "gen:routes": "ts-node src/routes.generate.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "cli": "ts-node src/cli/main.ts",
    "start": "ts-node src/index.ts",
    "build": "tsc && tsc-alias"
  },
  "author": "Shelby McCowan",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.10.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.10.0",
    "@types/passport": "^1.0.16",
    "@types/ws": "^8.5.13",
    "ts-morph": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@anthropic-ai/tokenizer": "^0.0.4",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "code-block-writer": "^13.0.3",
    "commander": "^12.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-defaults": "^5.0.2",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "figlet": "^1.8.0",
    "gpt-3-encoder": "^1.1.4",
    "helmet": "^8.0.0",
    "mongoose": "^8.9.3",
    "node-html-parser": "^6.1.13",
    "passport": "^0.7.0",
    "tsconfig-paths": "^4.2.0"
  }
}
